    <ImageView fx:id="welcome" fitHeight="664.0" fitWidth="1023.0" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@../Resources/StartPage.png"/>
        </image>
    </ImageView>
    <Button id="move_on_button" fx:id="beginButton" layoutX="470.0" layoutY="450.0" onAction="#moveOn"
            prefHeight="45.0" prefWidth="140.0"/>
    <ImageView fitHeight="242.0" fitWidth="679.0" layoutX="165.0" layoutY="69.0" pickOnBounds="true"
               preserveRatio="true">
        <image>
            <Image url="@../Resources/Title.png"/>
        </image>
    </ImageView>

</AnchorPane>
