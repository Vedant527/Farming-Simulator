<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="600.0" maxWidth="1000.0" minHeight="600.0" minWidth="1000.0" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FarmingSim.Controllers.MarketController">
   <children>
      <HBox minHeight="80.0" minWidth="1000.0" prefHeight="80.0" prefWidth="1000.0">
         <children>
            <HBox minWidth="800.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Text fx:id="farmName" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER">
                     <font>
                        <Font name="Constantia" size="54.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Text>
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="80.0" prefWidth="2000.0">
               <children>
                  <Button fx:id="farmButton" mnemonicParsing="false" onAction="#move_back" text="Go Farm!">
                     <font>
                        <Font name="Constantia" size="30.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" maxHeight="440.0" maxWidth="1000.0" minHeight="440.0" minWidth="1000.0" prefHeight="440.0" prefWidth="1000.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="2000.0" prefWidth="2000.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Buy:" textAlignment="CENTER">
                     <font>
                        <Font name="Constantia" size="48.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                     </VBox.margin>
                  </Text>
                  <HBox alignment="CENTER">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Corn">
                           <font>
                              <Font name="Constantia" size="30.0" />
                           </font>
                        </Text>
                        <Button fx:id="buyCornButton" mnemonicParsing="false" onAction="#buyCorn" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Wheat">
                           <font>
                              <Font name="Constantia" size="30.0" />
                           </font>
                        </Text>
                        <Button fx:id="buyWheatButton" mnemonicParsing="false" onAction="#buyWheat" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tobacco">
                           <font>
                              <Font name="Constantia" size="30.0" />
                           </font>
                        </Text>
                        <Button fx:id="buyTobaccoButton" mnemonicParsing="false" onAction="#buyTobacco" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Hemp">
                           <font>
                              <Font name="Constantia" size="30.0" />
                           </font>
                        </Text>
                        <Button fx:id="buyHempButton" mnemonicParsing="false" onAction="#buyHemp" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="2000.0" prefWidth="2000.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sell:" textAlignment="CENTER">
                     <font>
                        <Font name="Constantia" size="48.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                     </VBox.margin>
                  </Text>
                  <HBox alignment="CENTER">
                     <children>
                        <HBox>
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Corn">
                                 <font>
                                    <Font name="Constantia" size="30.0" />
                                 </font>
                              </Text>
                              <Button fx:id="sellCornButton" mnemonicParsing="false" onAction="#sellCorn" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Wheat">
                           <font>
                              <Font name="Constantia" size="30.0" />
                           </font>
                        </Text>
                        <Button fx:id="sellWheatButton" mnemonicParsing="false" onAction="#sellWheat" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tobacco">
                           <font>
                              <Font name="Constantia" size="30.0" />
                           </font>
                        </Text>
                        <Button fx:id="sellTobaccoButton" mnemonicParsing="false" onAction="#sellTobacco" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Hemp">
                           <font>
                              <Font name="Constantia" size="30.0" />
                           </font>
                        </Text>
                        <Button fx:id="sellHempButton" mnemonicParsing="false" onAction="#sellHemp" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" minHeight="80.0" minWidth="1000.0" prefHeight="80.0" prefWidth="1000.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="HUD:" textAlignment="CENTER" wrappingWidth="200.0">
               <font>
                  <Font name="Constantia" size="36.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Text>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="2000.0">
               <children>
                  <Text fx:id="seasonMarketDisplay" strokeType="OUTSIDE" strokeWidth="0.0">
                     <font>
                        <Font name="Constantia" size="36.0" />
                     </font>
                  </Text>
                  <Text fx:id="dayMarketDisplay" strokeType="OUTSIDE" strokeWidth="0.0" text="Day: 0" textAlignment="CENTER" wrappingWidth="200.0">
                     <font>
                        <Font name="Constantia" size="36.0" />
                     </font>
                     <HBox.margin>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Text>
                  <Text fx:id="maxSeedInventoryText" strokeType="OUTSIDE" strokeWidth="0.0">
                  <font>
                     <Font name="Constantia" size="12.0" />
                  </font>
               </Text>
                  <Text fx:id="moneyDisplay" strokeType="OUTSIDE" strokeWidth="0.0" text="Money: 0" textAlignment="CENTER" wrappingWidth="200.0">
                     <font>
                        <Font name="Constantia" size="36.0" />
                     </font>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </HBox.margin>
                  </Text>
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
</VBox>
